<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Manifest for AOMP 19.0-0 which uses ROCM 6.0 release branches of external repositories -->

    <remote name="gerritgit" review="git.amd.com:8080" fetch="ssh://gerritgit/" />
    <default revision="release/rocm-rel-6.0" remote="gerritgit" sync-j="4" sync-c="true" />
    <remote name="roctools"  fetch="https://github.com/ROCm/" />
    <remote name="roc"  fetch="https://github.com/ROCm/" />
    <remote name="rocsw"  fetch="https://github.com/ROCm/" />

    <project remote="roc" path="llvm-project" name="llvm-project" revision="aomp-19.0-0" groups="unlocked" />

    <project remote="roctools" path="flang" name="flang"               revision="aomp-19.0-0" groups="unlocked" />
    <project remote="roctools" path="aomp-extras" name="aomp-extras"   revision="aomp-19.0-0" groups="unlocked" />
    <project remote="roctools" path="aomp" name="aomp"                 revision="aomp-19.0-0" groups="unlocked" />

    <project remote="roctools" path="rocprofiler" name="rocprofiler"              revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roctools" path="roctracer" name="roctracer"                  revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roctools" path="ROCdbgapi" name="ROCdbgapi"                  revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roctools" path="ROCgdb" name="ROCgdb"                        revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roctools" path="hip" name="hip"                        revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roctools" path="clr" name="clr"                        revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roc" path="rocminfo" name="rocminfo"                         revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roc" path="rocm-cmake" name="rocm-cmake"                     revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roc" path="rocr-runtime" name="ROCR-Runtime"                 revision="rocm-6.0.2" groups="unlocked" />
    <project remote="roc" path="roct-thunk-interface" name="ROCT-Thunk-Interface" revision="rocm-6.0.2" groups="unlocked" />
    <project remote="rocsw" path="hipfort" name="hipfort"                         revision="rocm-6.0.2" groups="unlocked" />
</manifest>
